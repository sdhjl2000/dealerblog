@model DealerBlog.Entity.Post

<table class="table">
     
    <tr>
        <td style="width: 300px">
           @Html.ActionLink(Model.Title,"Details",new {Id=Model.Id})
        </td>
        <td>
            @Html.DisplayFor(model => model.PostedOn)
        </td>
  
        <td style="text-align:right">
            @Html.DisplayFor(model => model.BlogCategory.Name)
        </td>
       
    </tr>
 

</table>
